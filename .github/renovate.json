{
  "extends": [
    "config:recommended",
    "github>aquaproj/aqua-renovate-config#2.2.1"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    }
  ],
  "platformAutomerge": true,
  "ignoreTests": false
}
