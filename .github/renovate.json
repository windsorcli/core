{
  "extends": [
    "config:recommended",
    "github>aquaproj/aqua-renovate-config#2.6.0"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "addLabels": ["dependencies"]
    },
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true,
      "addLabels": ["dependencies"]
    }
  ],
  "platformAutomerge": true,
  "ignoreTests": false,
  "automerge": true
}
